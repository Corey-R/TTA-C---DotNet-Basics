@using CarInsurance.ViewModels
@model List<insureeVm>

<h2>Admin--Quotes</h2>
                                    
<table>
    <tr>
        <th>
            Quotes
        </th>
        <th>
            First Name
        </th>
        <th>
            Last Name
        </th>
        <th>
            Email Address
        </th>
    </tr>
    @foreach (var insuree in Model)
    {
        <tr>
            <td>
                @insuree.Quote
            </td>
            <td>
                @insuree.FirstName
            </td>
            <td>
                @insuree.LastName
            </td>
            <td>
                @insuree.EmailAddress
            </td>
        </tr>
    }
</table>                                         
      




